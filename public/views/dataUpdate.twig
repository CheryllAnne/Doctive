<div class="col-lg-6">
    <div class="card">
        <br/>{{ session.add }}{{ session.error }}<br/>
        <div class="card-header">
            <strong>Behabit-Doctive Data Update</strong>
        </div>
        <div class="card-body card-block">
            <form action="/v1/patient/habits" method="post" enctype="multipart/form-data" class="form-horizontal">
                {#                    <div class="row form-group">#}
                {#                        <div class="col col-md-3"><label class=" form-control-label">Static</label></div>#}
                {#                        <div class="col-12 col-md-9">#}
                {#                            <p class="form-control-static">Username</p>#}
                {#                        </div>#}
                {#                    </div>#}
                <div class="row form-group">
                    <div class="col col-md-3"><label for="text-input" class=" form-control-label">Full Name</label></div>
                    <div class="col-12 col-md-9"><input type="text" id="text-input" name="name" placeholder="Name" class="form-control"><small class="form-text text-muted">Patient's full name as per NRIC</small></div>
                </div>
                <div class="row form-group">
                    <div class="col col-md-3"><label for="text-input" class=" form-control-label">Behabit Username</label></div>
                    <div class="col-12 col-md-9"><input type="text" id="text-input" name="username" placeholder="Username" class="form-control"><small class="form-text text-muted">Behabit App Username</small></div>
                </div>
                <div class="row form-group">
                    <div class="col col-md-3"><label for="text-input" class=" form-control-label">Identification Card No.</label></div>
                    <div class="col-12 col-md-9"><input type="text" id="text-input" name="icNo" placeholder="IC No." class="form-control"><small class="form-text text-muted">IC number without '-'</small></div>
                </div>
                <div class="row form-group">
                    <div class="col col-md-3"><label for="email-input" class=" form-control-label">Age</label></div>
                    <div class="col-12 col-md-9"><input type="text" id="text-input" name="age" placeholder="" class="form-control"><small class="form-text text-muted">Age in this current year</small></div>
                </div>
                <div class="row form-group">
                    <div class="col col-md-3"><label for="select" class=" form-control-label">Sex</label></div>
                    <div class="col-12 col-md-9">
                        <select name="sex" id="select" class="form-control">
                            <option value="0">Please select</option>
                            <option value="Male">Male</option>
                            <option value="Female">Female</option>
                        </select>
                    </div>
                </div>
                <div class="row form-group">
                    <div class="col col-md-3"><label for="date-input" class=" form-control-label">Date</label></div>
                    <div class="col-12 col-md-9"><input type="date" id="text-input" name="date" placeholder="" class="form-control"><small class="form-text text-muted"></small></div>
                </div>
{#                <div class="row form-group">#}
{#                    <div class="col col-md-3"><label for="file-input" class=" form-control-label">File input</label></div>#}
{#                    <div class="col-12 col-md-9"><input type="file" id="file-input" name="file-input" class="form-control-file"></div>#}
{#                </div>#}
                <div class="card-footer">
                    <button type="submit" class="btn btn-primary btn-sm">
                        <i class="fa fa-dot-circle-o"></i> Submit
                    </button>
                    <button type="reset" class="btn btn-danger btn-sm">
                        <i class="fa fa-ban"></i> Reset
                    </button>
                </div>

            </form>

        </div>
        <div class="card-body card-block">
            <form action="/v1/patient/image" method="post" enctype="multipart/form-data" class="form-horizontal">
                <div class="row form-group">
                    <div class="col col-md-3"><label for="file-input" class=" form-control-label">File input</label></div>
                    <div class="col-12 col-md-9"><input type="file" id="file-input" name="file_input" class="form-control-file"></div>
                </div>
                <div class="card-footer">
                    <button type="submit" class="btn btn-primary btn-sm">
                        <i class="fa fa-dot-circle-o"></i> Upload
                    </button>
                </div>
            </form>
        </div>

    </div>

</div>
